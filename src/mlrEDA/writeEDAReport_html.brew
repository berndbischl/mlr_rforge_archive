<html>

<body>

<h3>Data characteristics</h3>
<%
  x = summarizeData(data, target)
  x1 = rev(x)[1]
  x = x[-length(x)]
  mode(x) = "integer"
  x = as.data.frame(t(x))
  x$class.quot = x1
  print(xtable(x), type="html")
%>  


<h3>Summary of features</h3>
<%
  print(xtable(summarizeColumns(data)), type="html")
%>  


<h3>Feature densities</h3>
<%
  s = ""	
  for (i in 1:ncol(data)) {
  	fn = sprintf("%s_feat_%i.png", name, i)
    png(file=fn)
  	print(plotFeatDistr(data, target, i))
    dev.off()
    s = paste(s,  "<img src='", fn, "'>", sep="")
  }	
%>  
<%=s%>  


<h3>Multi-Dimensional scaling</h3>
<%
  png(file=sprintf("%s_mds.png", name))
  p = plotMDS(data, target)
  print(p)
  dev.off()
%>
<img src="<%=name%>_mds.png">


<h3>PCA scree plot</h3>
<%
  png(file=sprintf("%s_pca.png", name))
  plotPCA(data, target)
  dev.off()
%>
<img src="<%=name%>_pca.png">


</body>
</html>


